<map
  id="map"
  latitude="{{location.latitude}}"
  longitude="{{location.longitude}}"
  scale="{{scale}}"
  setting="{{setting}}"
  markers="{{markers}}"
>
<view class="icon-container">
  <image class="round" 
  src="{{avatarURL? avatarURL: '/resources/account.png'}}" bindtap="onMyTripsTap"></image>
  <image 
  src="/resources/my_location.png"
  bindtap="onMyLocationTap"
  ></image>
</view>
<view class="btn-container bg-grey round" bindtap="onScanTap">扫码租车</view>
</map>